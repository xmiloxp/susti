<% include partes/_header %>
 
<div class="container mt-5">
    <div class="row">
        <div class="col-md-5">
            <div class="card">
                <div class="card-body">
                    <form action="/add/<%=data.codigo%>" method="post">
                        <div class="form-group">
                            <input type="text" name="codigo" value="<%= data.codio %>" class="form-control">
                        </div>
                        <div class="form-group">
                            <input type="text" name="altura" value="<%= data.altura %>" class="form-control">                              
                        </div>  
                        <div class="form-group">
                            <input type="text" name="tipo" value="<%= data.tipo %>" class="form-control">                              
                        </div>  
                        <div class="form-group">
                            <input type="text" name="longitud" value="<%= data.longitud %>" class="form-control">
                        </div>
                        <div class="form-group">
                            <input type="text" name="latitud" value="<%= data.latitud %>" class="form-control">
                        </div>
                        <button type="submit" class="btn btn-primary">
                            GUARDAR
                        </button>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<% include partes/_final %>

